#include <bits/stdc++.h>

using namespace std;

int main()
{
    cout<<"enter length of word";
    int v;
    cin>>v;
    string v1;
    for(int i=0;i<v;i++)
    {
        cin>>v1[i];
    }
     for(int i=0;i<v;i++)
    {
    if (v1[i] == 'a'||v1[i] == 'e'||v1[i] == 'i'||v1[i] == 'o'||v1[i] == 'u'||v1[i] == 'A'||v1[i] == 'E'||v1[i] == 'I'||v1[i] == 'O'||v1[i] == 'U')
    cout<<"it's a vowel"<<endl;
    else
    cout<<"it's a consonant"<<endl;
    }
    return 0;
}
