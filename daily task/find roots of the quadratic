
#include <bits/stdc++.h>
using namespace std;
int main()
{
  float a,b,c,d,r1,r2;
  cout<<"enter the coefficients"<<endl;
  cin>>a>>b>>c;
  d=(b*b)-(4*a*c);
  if(d>0)
  {
      r1=(-b+sqrt(d))/2*a;
      r2=(-b-sqrt(d))/2*a;
      cout<<"roots are real and unique "<<r1<<endl<<r2;
  }
  else if(d==0)
  {
      r1=r2=(-b)/2*a;
      cout<<"roots are equal "<<r1;
  }
  else
  cout<<"roots are imaginary";
  return 0;
}
