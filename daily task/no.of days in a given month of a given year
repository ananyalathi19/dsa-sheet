
#include <bits/stdc++.h>
using namespace std;
int main()
{
  int m,y;
  cout<<"enter month and year"<<endl;
  cin>>m>>y;
    if((m==2 && y%400==0) || (m==2&&y%100!=0 && y%4==0))
    cout<<"29days"<<endl;
    else if(m==1||m==3||m==5||m==7||m==8||m==10||m==12)
    cout<<"31days"<<endl;
    else if (m==4||m==6||m==9||m==11)
    cout<<"30days"<<endl;
    else
    cout<<"28days"<<endl;
    return 0;
}
