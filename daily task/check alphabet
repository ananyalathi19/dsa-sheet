#include <bits/stdc++.h>

using namespace std;

int main()
{
    cout<<"enter length of word";
    int v;
    cin>>v;
    string v1;
    for(int i=0;i<v;i++)
    {
        cin>>v1[i];
    }
     for(int i=0;i<v;i++)
    {
    if ((v1[i] >= 'a'&& v1[i] <= 'z')||(v1[i] >= 'A' && v1[i] <= 'Z'))
    cout<<"it's an alphabet"<<endl;
    else
    cout<<"not an alphabet"<<endl;
    }
    return 0;
}
